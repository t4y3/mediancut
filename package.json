{
  "name": "mediancut",
  "version": "2.1.0",
  "description": "Image reduction by Mediancut algorithm",
  "main": "lib/mediancut.js",
  "module": "lib/mediancut.esm.js",
  "unpkg": "lib/mediancut.js",
  "types": "lib/mediancut.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "prepublishOnly": "rm -rf lib && npm run build && npm run types",
    "build": "npm run clean && rollup -c && npm run types",
    "watch": "rollup -c -w",
    "types": "tsc -d --emitDeclarationOnly"
  },
  "files": [
    "lib"
  ],
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint src",
      "prettier --write"
    ]
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit $1",
    "pre-commit": "npx lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t4y3/mediancut.git"
  },
  "keywords": [
    "mediancut",
    "gradation",
    "canvas"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/t4y3/mediancut/issues"
  },
  "homepage": "https://github.com/t4y3/mediancut#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rollup": "^2.37.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "semantic-release": "^19.0.3",
    "simple-git-hooks": "^2.2.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {}
}
